{
  "name": "brdev/framework",
  "type": "project",
  "description": "The BRdev Framework, created to implements projects the big value",
  "keywords": ["framework", "coffecode"],
  "license": "MIT",
  "authors": [
    {
      "name": "Brendo O O santos",
      "email": "brendo.dev@outlook.com",
      "role": "Developer"
    }
  ],
  "config": {
    "vendor-dir": "vendor"
  },
  "autoload": {
    "psr-4": {
      "Source\\": "source/"
    },
    "files": [
      "source/Boot/Config.php",
      "source/Boot/Helpers.php"
    ]
  },
  "require": {
    "php": ">=8.0",
    "ext-pdo": ">=8.0",
    "phpmailer/phpmailer": "6.*",
    "league/plates": "3.*",
    "coffeecode/uploader": "2.*",
    "coffeecode/cropper": "2.*",
    "coffeecode/optimizer": "2.*",
    "coffeecode/paginator": "2.*",
    "coffeecode/router": "2.*",
    "symfony/var-dumper": "6.*",
    "filp/whoops": "2.*",
    "monolog/monolog": "3.*",
    "aws/aws-sdk-php": "^3.252"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
